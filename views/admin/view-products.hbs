<section>
    <div class="container">
        <div class="row my-3">
            <div class="col-md-9">

            </div>
        <div class="col-md-3">
            <a href="/admin/add-products" class=" btn btn-success">add products</a>
        </div>
            
        </div>
        <table class="table">
  <thead>
    <tr>
      <th scope="col">no</th>
      <th scope="col">Title </th>
      <th scope="col">catogory</th>
      <th scope="col">Description</th>
      <th scope="col">image</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.catogory}}</td>
      <td>{{this.description}}</td>
      <td><img style="width: 50px;height:50px" src="{{this.image}}"</td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>